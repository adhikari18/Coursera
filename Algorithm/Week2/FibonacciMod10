using System;

namespace Week2
{
    //FibonacciMod10
    //Last Digit of the Sum of Fibonacci Numbers

    public class FibonacciMod10
    {
        static void Main(string[] args)
        {
            long n = long.Parse(Console.ReadLine());
            long fibN = GetFibonacciModulo(n, 10);
            long fibNPlus1 = GetFibonacciModulo(n+1, 10);
            Console.WriteLine((fibN + fibNPlus1 - 1) % 10);
        }

        static long GetPisanoPeriod(long m)
        {
            long a = 0, b = 1, c = a + b;
            for (int i = 0; i < m * m; i++)
            {
                c = (a + b) % m;
                a = b;
                b = c;
                if (a == 0 && b == 1)
                {
                    return i + 1;
                }
            }
            return 0;
        }

        static long GetFibonacciModulo(long n, long m)
        {
            long remainder = n % GetPisanoPeriod(m);
            long first = 0;
            long second = 1;

            long temp = remainder;

            for (int i = 1; i < remainder; i++)
            {
                temp = (first + second) % m;
                first = second;
                second = temp;
            }

            return temp % m;
        }
    }
}
